#% Do not delete the line below; configure depends on this
#  \DeclareLaTeXClass{UTU FT Lab report}
#  \DeclareCategory{Articles}
# UTU FT lab report document class, based on report
# Author : Jari-Matti Mäkelä <jmjm@iki.fi>

Format 60
Input stdclass.inc

SecNumDepth            3
TocDepth               3

Provides fontenc       1
Provides url           1
Provides geometry      1
Provides babel         1
Provides amsmath       1
Provides amssymb       1
Provides fontspec      1
Provides graphicx      1
Provides biblatex      1
Provides hyperref      1

Style Appendix
    CopyStyle     Chapter
    LatexName     appchapter
    LabelString   "Appendix" # For the menu?
End

Style PubYear
    Margin                Dynamic
    LatexType             Command
    LatexName             pubyear
    LabelType   Static
    LabelString           "Publication year:"
    Align                 Center
    Font
      Size                Large
    EndFont
End

Style PubMonth
    Margin                Dynamic
    LatexType             Command
    LatexName             pubmonth
    LabelType   Static
    LabelString           "Publication month: "
    Align                 Center
    Font
      Size                Large
    EndFont
End

Style PubLab
    Margin                Dynamic
    LatexType             Command
    LatexName             publab
    LabelType   Static
    LabelString           "Laboratory name: "
    Align                 Center
    Font
      Size                Large
    EndFont
End

Style PubCourse
    Margin                Dynamic
    LatexType             Command
    LatexName             pubcourse
    LabelType   Static
    LabelString           "Course name: "
    Align                 Center
    Font
      Size                Large
    EndFont
End

Style GroupMembers
    Margin                Dynamic
    LatexType             Command
    LatexName             groupmembers
    LabelType   Static
    LabelString           "Group members: "
    Align                 Center
    Font
      Size                Large
    EndFont
End

Style Javacode
    CopyStyle     Verbatim
    LatexName     javacode
    LabelString   "Javacode\n" # For the menu?
End

Style Part
    Align                 Left
    AlignPossible         Left
    TopSep                2
    BottomSep             1.5
    Font
      Size                Larger
    EndFont
End

Style Part*
    Align                 Left
    AlignPossible         Left
    TopSep                2
    BottomSep             1.5
    Font
      Size                Larger
    EndFont
End
